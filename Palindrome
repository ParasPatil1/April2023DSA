 int PalinArray(int a[], int n)
    {
        for (int i=0;i<n;i++)
        {
            int num=0;
            int p=a[i];
            while (p!=0)
            {
                num=num*10+p%10;
                p=p/10;
            }
            if(a[i]!=num)
            {
                return 0;
            }
           
        }   
        return 1; 
    }
