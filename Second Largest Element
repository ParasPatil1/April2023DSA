	int print2largest(int arr[], int n) 
{ 
    if (n<2) {
        return -1;
    }
    int larg = INT_MIN;
    int larg2 = INT_MIN;
   
   for (int i=0;i<n;i++)
   {
      if (arr[i]>larg)
      {
         larg2=larg;
         larg=arr[i];
      }
      else if (arr[i]>larg2 && arr[i] != larg)
      {
         larg2=arr[i];
      }
      
   }
      if (larg2 == INT_MIN)
	        return -1;
	    else
	        return larg2;
}
};
